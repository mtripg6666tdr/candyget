  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtripg6666tdr/candyget.git"
  },
  "bugs": {
    "url": "https://github.com/mtripg6666tdr/candyget/issues"
  },
  "homepage": "https://github.com/mtripg6666tdr/candyget#readme",
  "peerDependencies": {
    "@candyget/browser": "*"
  },
  "peerDependenciesMeta": {
    "@candyget/browser": {
      "optional": true
    }
  }
}
