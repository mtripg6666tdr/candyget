{
  "name": "candyget",
  "version": "0.6.0-beta.0",
  "description": "A tiny, candy sized HTTP(S) client for Node.js",
  "main": "./index.js",
  "browser": "./browser.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha -- --require ts-node/register test/*.test.ts",
    "build": "swc src -d .",
    "lint": "eslint .",
    "cleanup": "rimraf index.js index.d.ts tsconfig.tsbuildinfo"
