{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": false,
      "decorators": false
    },
    "transform": {},
    "target": "es2019",
    "loose": true,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": true,
    "minify": {
      "compress": {},
      "mangle": {
        "keepClassNames": true
      }
    }
  },
  "minify": true,
  "module": {
    "type": "commonjs"
  }
} 
